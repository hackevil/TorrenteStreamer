  "dependencies": {
    "express": "^4.14.1",
    "jade": "^1.11.0",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "nodemon": "^1.9.2",
    "stylus": "^0.54.5",
    "webtorrent": "^0.98.7"
